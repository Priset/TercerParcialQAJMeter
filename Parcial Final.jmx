        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="Create  BASE64" enabled="true">
          <stringProp name="BeanShellSampler.query">import org.apache.commons.codec.binary.Base64;

log.info(&quot;I INFO &gt; creating base64&quot;);

String credentials = vars.get(&quot;EMAIL&quot;) + &quot;:&quot; + vars.get(&quot;PASSWORD&quot;);

byte[] credentialsConverted = Base64.encodeBase64(credentials.getBytes());


vars.put(&quot;BASE64&quot;,&quot;Basic &quot; + new String(credentialsConverted));

log.info(&quot; &gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt; &quot; + vars.get(&quot;BASE64&quot;));</stringProp>
          <boolProp name="BeanShellSampler.resetInterpreter">false</boolProp>
